    "plt.xlim(-5, 5)\n",
    "plt.ylim(-2, 2)\n",
    "\n",
    "plt.title('step function')\n",
    "plt.xlabel('t')\n",
    "plt.ylabel('u(t)')\n",
    "plt.grid(True)\n"
